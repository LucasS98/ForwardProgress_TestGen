macro is_in (t, f) =
  ( index (f, concat (concat (":", string (t)), ":")) <> 0 )
end_macro
< true* . { EX ... ?fairlyScheduled:String } >
<
    true .
    
    for tid:Nat from 0 to 4 do
        if is_in (tid, fairlyScheduled) then
            true* . { EX !tid ... }
        end if
    end for
> @
